{
  "name": "jest-date",
  "version": "1.0.1",
  "description": "Custom jest matchers to test dates",
  "main": "dist/index.js",
  "repository": "git@github.com:Stefanwullems/jest-date.git",
  "author": "stefanwullems <stefan@exivity.com>",
  "license": "MIT",
  "private": false,
  "keywords": [
    "date",
    "date-fns",
    "jest"
  ],
  "devDependencies": {
    "kcd-scripts": "5.6.0",
    "pretty-format": "^25.4.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "date-fns": "^2.12.0",
    "jest-matcher-utils": "^25.4.0"
  },
  "scripts": {
    "build": "kcd-scripts build",
    "build:docs": "yarn doctoc . --maxlevel 3 --notitle",
    "format": "kcd-scripts format",
    "lint": "kcd-scripts lint",
    "setup": "npm install && npm run validate -s",
    "test": "kcd-scripts test",
    "test:update": "npm test -- --updateSnapshot --coverage",
    "validate": "kcd-scripts validate"
  },
  "files": [
    "dist",
    "extend-expect.js",
    "matchers.js"
  ],
  "eslintConfig": {
    "extends": "./node_modules/kcd-scripts/eslint.js",
    "rules": {
      "babel/no-invalid-this": "off"
    }
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "dist"
  ]
}
